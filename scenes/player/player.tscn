[sub_resource type="Shader" id="Shader_dbc6n"]
code = "// NOTE: Shader automatically converted from Godot Engine 3.4.stable's StandardMaterial3D.

shader_type spatial;
render_mode blend_mix,depth_draw_opaque,cull_disabled,diffuse_burley,specular_schlick_ggx,unshaded;//depth_test_disable;

uniform float ext1 : hint_range(0,1) = 0.25;
uniform float ext2 : hint_range(0,1) = 0.75;

uniform vec4 albedo : source_color;
uniform sampler2D texture_albedo : source_color;
uniform float specular;
uniform float metallic;
uniform float roughness : hint_range(0,1);
uniform float point_size : hint_range(0,128);
uniform sampler2D texture_emission : hint_default_black;
uniform vec4 emission : source_color;
uniform float emission_energy;
uniform vec3 uv1_scale;
uniform vec3 uv1_offset;
uniform vec3 uv2_scale;
uniform vec3 uv2_offset;

float remap( float f, float start1, float stop1, float start2, float stop2){
	return start2 + (stop2 - start2) * ((f - start1) / (stop1 - start1));
}


void vertex() {
	UV=UV*uv1_scale.xy+uv1_offset.xy;
}


void fragment() {
	vec2 base_uv = UV;
	vec4 albedo_tex = texture(texture_albedo,base_uv);
	ALBEDO = albedo.rgb * albedo_tex.rgb;
	METALLIC = metallic;
	ROUGHNESS = roughness;
	SPECULAR = specular;
	vec3 emission_tex = texture(texture_emission,base_uv).rgb;
	EMISSION = (emission.rgb+emission_tex)*emission_energy;
	float a = albedo.a *
		albedo_tex.a *
		clamp(remap( UV.x, ext1, ext1+.0001, 0.0, 1.0  ), 0.25, 1.0) * 
		clamp(remap( UV.x, ext2, ext2-.0001, 0.0, 1.0  ), 0.25, 1.0); 
	ALPHA = a;
}
"

